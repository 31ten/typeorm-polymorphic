{
  "name": "typeorm-polymorphic",
  "version": "0.0.2",
  "main": "./dist/index.js",
  "author": {
    "name": "Ashleigh Simonelli",
    "email": "ashleighsimonelli@gmail.com",
    "url": "http://ashleighsimonelli.co.uk"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bashleigh/typeorm-polymorphic"
  },
  "description": "A simple pagination function to build a pagination object with types",
  "keywords": [
    "nestjs",
    "typeorm",
    "polymorphic",
    "relationships",
    "decorators",
    "mysql"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "build": "rm -rf ./dist && tsc && npm run build:index",
    "build:index": "rm -rf ./index.js ./index.d.ts && tsc -d --skipLibCheck ./index.ts",
    "prepublish": "npm run format && npm run build"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.29",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "typeorm": "^0.2.29"
  }
}
